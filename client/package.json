{
    "name": "EssaySpring",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "test:unit": "vue-cli-service test:unit",
        "test:unit:watch": "vue-cli-service test:unit --watch --verbose true",
        "dev": "nuxt",
        "generate": "nuxt generate",
        "lint": "eslint --ext .js,.ts,.vue .",
        "lintfix": "eslint --fix --ext .js,.ts,.vue .",
        "start": "nuxt start",
        "docs": "npx jsdoc -c ./jsdoc.json"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.13.1",
        "@nuxtjs/pwa": "^3.3.5",
        "axios": "^0.25.0",
        "cookie": "^0.4.1",
        "core-js": "^3.6.5",
        "dotenv": "^16.0.0",
        "install": "^0.13.0",
        "js-cookie": "^2.2.1",
        "mqtt": "^4.2.6",
        "npm": "^8.4.1",
        "nuxt": "^2.15.3",
        "nuxt-compress": "^5.0.0",
        "nuxt-material-design-icons-iconfont": "^1.0.2",
        "pdfjs-dist": "^2.10.377",
        "register-service-worker": "^1.7.1",
        "secure-ls": "^1.2.6",
