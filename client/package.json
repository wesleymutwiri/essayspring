        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/vuetify": "^1.11.3",
        "@types/jest": "^24.0.19",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "@vue/cli-plugin-babel": "~4.5.0",
