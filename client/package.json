        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.3.0",
        "axios-mock-adapter": "^1.20.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "css-loader": "^4.2.0",
        "cypress": "^9.4.1",
        "eslint": "^7.32.0",
